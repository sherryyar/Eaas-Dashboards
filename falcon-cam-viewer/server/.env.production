# OpsGenie Configuration
OPSGENIE_API_KEY=your_opsgenie_key

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info 