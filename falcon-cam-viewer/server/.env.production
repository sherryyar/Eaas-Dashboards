# Server Configuration
PORT=3001
NODE_ENV=production

# Frontend URL (for CORS)
FRONTEND_URL=https://your-frontend-domain.com

# Slack Configuration
SLACK_TOKEN=your_slack_token
SLACK_CHANNEL_ID=your_channel_id
